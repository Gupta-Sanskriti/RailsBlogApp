<h1>Articles</h1>

<ul>
  <!-- this article is coming from the articles 'articles_controller.rb' file -->
  <% @articles.each do |article| %>
  <li>
    <a
      href="/articles/<%= article.id %>"
      style="text-decoration: none; color: black">
      <%= article.title %>
    </a>
  </li>
  <% end %>
</ul>
